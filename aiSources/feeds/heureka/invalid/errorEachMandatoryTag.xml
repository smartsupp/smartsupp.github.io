<!-- wp:uagb/container {"block_id":"4a0546f2","innerContentCustomWidthDesktop":1200,"directionDesktop":"row","alignItemsDesktop":"flex-start","topPaddingDesktop":64,"bottomPaddingDesktop":64,"leftPaddingDesktop":48,"rightPaddingDesktop":48,"topPaddingTablet":48,"bottomPaddingTablet":48,"leftPaddingTablet":48,"rightPaddingTablet":48,"topPaddingMobile":24,"bottomPaddingMobile":24,"leftPaddingMobile":24,"rightPaddingMobile":24,"variationSelected":true,"isBlockRootParent":true} -->
